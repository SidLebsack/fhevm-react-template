{
  "name": "fhevm-universal-sdk",
  "version": "1.0.0",
  "description": "Universal FHEVM SDK for building confidential dApps with Next.js, React, Vue, and Node.js",
  "private": true,
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "install:all": "npm install && npm run install:packages && npm run install:examples",
    "install:packages": "cd packages/fhevm-sdk && npm install",
    "install:examples": "npm run install:nextjs && npm run install:fitness",
    "install:nextjs": "cd examples/nextjs-fitness-tracker && npm install",
    "install:fitness": "cd examples/privacy-fitness-tracker && npm install",
    "build": "npm run build:sdk && npm run build:examples",
    "build:sdk": "cd packages/fhevm-sdk && npm run build",
    "build:examples": "npm run build:nextjs",
    "build:nextjs": "cd examples/nextjs-fitness-tracker && npm run build",
    "dev": "npm run dev:nextjs",
    "dev:nextjs": "cd examples/nextjs-fitness-tracker && npm run dev",
    "dev:fitness": "cd examples/privacy-fitness-tracker && npm run deploy:local",
    "compile:contracts": "cd examples/privacy-fitness-tracker && npm run compile",
    "deploy:local": "cd examples/privacy-fitness-tracker && npm run deploy:local",
    "deploy:sepolia": "cd examples/privacy-fitness-tracker && npm run deploy",
    "test": "npm run test:sdk && npm run test:contracts",
    "test:sdk": "cd packages/fhevm-sdk && npm test",
    "test:contracts": "cd examples/privacy-fitness-tracker && npm test",
    "lint": "npm run lint:sdk && npm run lint:examples",
    "lint:sdk": "cd packages/fhevm-sdk && npm run lint",
    "lint:examples": "cd examples/nextjs-fitness-tracker && npm run lint",
    "clean": "npm run clean:sdk && npm run clean:examples",
    "clean:sdk": "cd packages/fhevm-sdk && rm -rf dist node_modules",
    "clean:examples": "cd examples/nextjs-fitness-tracker && rm -rf .next node_modules",
    "setup": "npm install && npm run build:sdk && npm run compile:contracts"
  },
  "keywords": [
    "fhevm",
    "zama",
    "fhe",
    "encryption",
    "privacy",
    "blockchain",
    "ethereum",
    "sdk",
    "nextjs",
    "react",
    "vue",
    "nodejs",
    "web3",
    "confidential-computing"
  ],
  "author": "FHEVM SDK Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/fhevm-universal-sdk"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "prettier": "^3.0.0",
    "eslint": "^8.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
